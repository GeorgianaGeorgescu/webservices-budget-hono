{
  "compilerOptions": {
    "noEmit": true,
    "target": "ESNext",
    "module": "commonjs",
    "esModuleInterop": true,    
    "strict": true,
    "verbatimModuleSyntax": false,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
  },
  "include": [
    "**/*.ts"
  ],
    "exclude": [
    "jest.config.ts",
    "__tests__"
  ]
}